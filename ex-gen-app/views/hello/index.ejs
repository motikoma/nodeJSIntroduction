<html lang="ja">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>
    <header><h1><%= title %></h1></header>
    <div role="main">
        <% if(content != null) { %>
            <table>
            <% for(val i in content) { %>
                <tr>
                    <% var obj = content[i]; %>
                        <td><%= obj.name %></td>
                        <td><%= obj.mail %></td>
                        <td><%= obj.age %></td>
                </tr>
            <% } %>
            </table>
        <% } %>
    </div>
</body>
</html>